	"tokenColors": [
		{
			"name": "RED",
			"scope": [
				"constant.numeric",
				"constant.language.true",
				"constant.language.false",
				"constant.language.nullptr",
				"punctuation.section.block",
				"keyword.operator.cast.const_cast"
				

				// python
				// ---
				
			],
			"settings": {
				"foreground": "#AC5A42"
			}
		},
		{
			"name": "GOLD",
			"scope": [
				// python
				// ---
				
			],
			"settings": {
				"foreground": "#E1B655"
			}
		},
		{
			"name": "PURPLE",
			"scope": [
				"keyword.control.if",
				"keyword.control.else",
				"keyword.control.for",
				"keyword.control.while",
				"keyword.control.return",
				"keyword.control.break",
				"keyword.control.continue",
				"keyword.control.try",
				"keyword.control.catch",
				"keyword.control.throw",
				"storage.modifier.specifier.const",
				"storage.modifier.const",
				"storage.modifier.specifier.extern",
				"storage.modifier.specifier.functional.post-parameters",
				"storage.modifier.specifier.functional.pre-parameters",
				"storage.modifier.virtual",
				"storage.modifier.static",
				"storage.modifier.inline",
				"storage.modifier.specifier.static",
				"storage.type.modifier.access.control",
				"storage.type.namespace",
				"storage.type.class",
				"storage.type.struct",
				"storage.type.enum",
				"storage.type.template",
				"storage.type.template.argument.typename",
				"storage.type.extern",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.other.delete",
				"keyword.other.typedef",
				"keyword.other.default",
				"keyword.other.using",

				// python
				"keyword.control.flow"

			],
			"settings": {
				"foreground": "#E18BE1"
			}
		},
		{
			"name": "BLUE",
			"scope": [
				"entity.name.function",
				"keyword.operator.cast",

				// python
				"meta.function-call"

			],
			"settings": {
				"foreground": "#8FA5CD"
			}
		},
		{
			"name": "GREEN",
			"scope": [
				"string",
				"punctuation.definition.string"

				// python
				// ---
				
			],
			"settings": {
				"foreground": "#99AD6A"
			}
		},
		{
			"name": "GREEN but different",
			"scope": [
				"constant.character.escape",
				"constant.other.placeholder"

				// python
				// ---
				
			],
			"settings": {
				"foreground": "#69BB69BB"
			}
		},
		{
			"name": "LILAC",
			"scope": [
				"storage.type.built-in.primitive",
				"keyword.control.directive",
				"keyword.other.unit.suffix.floating-point",

				// python
				"keyword.control.import",
				"storage.type.function",

				// cmake
				"keyword.cmake"
				
			],
			"settings": {
				"foreground": "#C6B5DA"
			}
		},
		{
			"name": "WHITE",
			"scope": [
				"keyword.operator",
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.definition",
				"punctuation.section",
				"storage.modifier.reference",
				"storage.modifier.pointer",
				"variable.other",
				"variable.language.this",

				// python
				// ---

				// cmake
				"entity.source.cmake",
				"variable.source.cmake",
				"source.cmake"

			],
			"settings": {
				"foreground": "#D9D9C4"
			}
		},
		{
			"name": "WHITE but different",
			"scope": [
				

				// python
				// ---

				// cmake
				"storage.source.cmake"

			],
			"settings": {
				"foreground": "#BBBB84"
			}
		},
		{
			"name": "GRAY",
			"scope": [
				"comment",
				"punctuation.definition.comment"

				// python
				// ---
				
			],
			"settings": {
				"foreground": "#7F6666",
				"fontStyle": "italic"
			}
		},
		{
			"name": "JSON - WHITE",
			"scope": [
				"support.type.property-name.json.comments"
			],
			"settings": {
				"foreground": "#bfbdb6"
				
